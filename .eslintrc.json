{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint-config-airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "eslint-config-prettier"
  ],
  "plugins": [
    "eslint-plugin-prettier"
  ],
  "parserOptions": {},
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": true,
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 80
      }
    ],
    "@typescript-eslint/indent": "off",
    "import/prefer-default-export": "off"
  }
}
